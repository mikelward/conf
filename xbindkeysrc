#
# A list of keys is in /usr/include/X11/keysym.h and in
# /usr/include/X11/keysymdef.h
# The XK_ is not needed.
#
# List of modifier:
#   Release, Control, Shift, Mod1 (Alt), Mod2 (NumLock),
#   Mod3 (CapsLock), Mod4, Mod5 (Scroll).
#

"gnome-calculator"
	Mod4 + c

"firefox"
	Mod4 + f

"chrome"
	Mod4 + g

# h = home folder
#"thunar"
"nautilus ~"
	Mod4 + h

"irc"
	Mod4 + i

# l = lock screen
"lock-screensaver"
	Mod4 + l

# m = mail
"gmail"
	Mod4 + m

"notepad"
	Mod4 + n

# o = opera
"opera"
    Mod4 + o

# p = play music
"music"
	Mod4 + p

"remote-desktop"
    Mod4 + r

# s = shell
"shell"
	Mod4 + s

# t = terminal
"terminal"
	Mod4 + t

"code"
	Mod4 + v

# mute
"vol mute"
	Mod4 + KP_Delete

"vol mute"
	Mod4 + Delete

"vol mute"
    XF86AudioMute

# play/pause
"media playpause"
	Mod4 + KP_Insert

"media playpause"
    XF86AudioPlay

"media playpause"
	Mod4 + Insert

# volume down
"vol down"
	Mod4 + Down

"vol down"
    XF86AudioLowerVolume

# volume up
"vol up"
	Mod4 + Up

"vol up"
    XF86AudioRaiseVolume

# next song
"media next"
	Mod4 + Right

# previous song
"media prev"
	Mod4 + Left

# screen shot (print screen)
#"gnome-screenshot"
"xfce4-screenshooter -f"
	Print

"snap left"
    Mod4 + Shift + Left

"snap right"
    Mod4 + Shift + Right

"moveto 1"
    Mod4 + Shift + 1

"moveto 2"
    Mod4 + Shift + 2

"moveto 3"
    Mod4 + Shift + 3

"moveto 4"
    Mod4 + Shift + 4
