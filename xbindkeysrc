#
# A list of keys is in /usr/include/X11/keysym.h and in
# /usr/include/X11/keysymdef.h
# The XK_ is not needed.
#
# List of modifier:
#   Release, Control, Shift, Mod1 (Alt), Mod2 (NumLock),
#   Mod3 (CapsLock), Mod4, Mod5 (Scroll).
#

"blueman-manager"
    Mod4 + b

"google-calendar"
    Mod4 + c

"code"
    Mod4 + d

"gnome-calculator"
	Mod4 + Shift + c

"gnome-calculator"
	XF86Calculator

"editor"
    Mod4 + e

"browser2"
	Mod4 + f

"browser1"
	Mod4 + g

"browser3"
	Mod4 + Shift + g

#"home"
#	Mod4 + h

"irc"
	Mod4 + i

"lock-screensaver"
	Mod4 + l

"google-meet"
	Mod4 + m

"external_monitors_only"
    Mod4 + Shift + m

"notepad"
	Mod4 + n

# p = play music
"music"
	Mod4 + p

"xfce4-display-settings"
    Mod4 + Shift + d

"remote-desktop"
    Mod4 + r

"secure-shell"
    Mod4 + s

"terminal"
	Mod4 + t

"terminal_on_workstation"
    Mod4 + w

"workspaces"
    Mod4 + Shift + w

"youtube-music"
    Mod4 + y

"vol mute"
    XF86AudioMute

"playpause"
    Release+XF86AudioPlay

"vol down"
    XF86AudioLowerVolume

"vol up"
    XF86AudioRaiseVolume

"screenshot -f"
	Print

"screenshot -w"
    Alt + Print
