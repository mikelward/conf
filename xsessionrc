log() {
  echo "$(date +"%F %T.%N") $*" >> "$HOME/xsession.log"
}

log ".xsessionrc"
setxkbmap -model pc104 -layout us -variant dvorak
. "$HOME/.shrc"
external_monitors_only
export GTK_OVERLAY_SCROLLING=0
screensaver init
imwheel
xbindkeys
(setsid nm-applet&)
eval "$(ssh-agent)"
xsetroot -bg black
xsetroot -cursor_name left_ptr
